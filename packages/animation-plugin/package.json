{
  "name": "@crossed/animation-plugin",
  "version": "1.0.0",
  "description": "A crossed plugin for animation properties, utilizing animation libraries.",
  "keywords": [
    "react",
    "native",
    "react-native",
    "animation",
    "transition"
  ],
  "main": "lib/commonjs/index",
  "types": "lib/typescript/index.d.ts",
  "module": "lib/module/index",
  "react-native": "src/index",
  "source": "src/index",
  "typings": "lib/typescript/index.d.ts",
  "scripts": {
    "prepare": "bob build",
    "release": "release-it",
    "watch": "crossed-build --watch",
    "build": "crossed-build",
    "clean": "rm -rf lib"
  },
  "dependencies": {
    "@legendapp/motion": "^2.2.0"
  },
  "devDependencies": {
    "@crossed/core": "^1.0.0",
    "@types/react": "^18.0.22",
    "@types/react-native": "^0.69.15",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-native": "^0.70.3",
    "@crossed/build": "1.0.0",
    "tsconfig": "*",
    "typescript": "^4.7.4"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      [
        "module"
      ],
      "typescript"
    ]
  },
  "files": [
    "lib/",
    "src/"
  ]
}
